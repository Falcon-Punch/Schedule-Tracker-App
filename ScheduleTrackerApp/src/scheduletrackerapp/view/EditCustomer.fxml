<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scheduletrackerapp.view.EditCustomerController">
   <children>
      <GridPane layoutX="9.0" layoutY="9.0" prefHeight="400.0" prefWidth="360.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="203.0" minWidth="10.0" prefWidth="176.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="220.0" minWidth="10.0" prefWidth="184.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Name">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <TextField fx:id="nameTextField" GridPane.columnIndex="1" />
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonClicked" prefWidth="90.0" stylesheets="@EditCustomer.css" text="Save" GridPane.rowIndex="7" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonClicked" prefWidth="90.0" stylesheets="@EditCustomer.css" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Label text="Country" GridPane.rowIndex="5">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <Label text="Address 2" GridPane.rowIndex="3">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <TextField fx:id="countryTextField" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="address2TextField" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label text="Address" GridPane.rowIndex="2">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <Label text="Phone" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <Label text="City" GridPane.rowIndex="4">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <Label text="Postal Code" GridPane.rowIndex="6">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font></Label>
            <TextField fx:id="addressTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="phoneTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="cityTextField" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="postalCodeTextField" GridPane.columnIndex="1" GridPane.rowIndex="6" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
